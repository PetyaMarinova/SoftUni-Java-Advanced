package Ex11Threeuple;

public class Tuple<K, V, T> {
    private K item1;
    private V item2;
    private T item3;

    public Tuple(K item1, V item2, T item3){
        this.item1 = item1;
        this.item2 = item2;
        this.item3 = item3;
    }

    @Override
    public String toString() {
        return String.format("%s -> %s -> %s",this.item1, this.item2, this.item3);
    }
}
