package Ex07CustomList;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        String input = scanner.nextLine();
        CustomList<String> customList = new CustomList<String>();

        while(!input.equals("END")){
            String[] tokens = input.split("\\s+");
            switch (tokens[0]){
                case "Add":
                    String element = tokens[1];
                    customList.add(element);
                    break;
                case "Remove":
                    int indexToRemove = Integer.parseInt(tokens[1]);
//                    if(customList.isIndexValid(indexToRemove)){
                        String removedElement = customList.remove(indexToRemove);
//                    }
                    break;
                case "Contains":
                    String containsElement = tokens[1];
                    if (customList.contains(containsElement)){
                        System.out.println("true");
                    } else {
                        System.out.println("false");
                    }
                    break;
                case "Swap":
                    int index1 = Integer.parseInt(tokens[1]);
                    int index2 = Integer.parseInt(tokens[2]);
//                    if (customList.isIndexValid(index1) && customList.isIndexValid(index2)){
                        customList.swap(index1, index2);
//                    }
                    break;
                case "Greater":
                    System.out.println(customList.countGreaterThan(tokens[1]));
                    break;
                case "Max":
                    System.out.println(customList.getMax());
                    break;
                case "Min":
                    System.out.println(customList.getMin());
                    break;
                case "Print":
                    System.out.println(customList);
                    break;

            }

            input = scanner.nextLine();
        }
    }
}
//•	Add <element> - Adds the given element to the end of the list
//•	Remove <index> - Removes the element at the given index
//•	Contains <element> - Prints if the list contains the given element (true or false)
//•	Swap <index> <index> - Swaps the elements at the given indexes
//•	Greater <element> - Counts the elements that are greater than the given element and prints their count
//•	Max - Prints the maximum element in the list
//•	Min - Prints the minimum element in the list
//•	Print - Prints all elements in the list, each on a separate line
//•	END - stops the reading of commands
